{
    "name": "sergeybastrikov/oauthVk",
    "type": "library",
    "description": "VKontakte OAuth2 Provider",
    "keywords": [
        "sergeybastrikov",
        "oauthVk"
    ],
    "homepage": "https://github.com/sergeybastrikov/oauthVk",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Bastrikov",
            "email": "sergey@bastrikov.com",
            "homepage": "https://bastrikov.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "sergeybastrikov\\oauthVk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sergeybastrikov\\oauthVk\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
